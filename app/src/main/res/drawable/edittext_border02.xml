<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#F3F3F3"/>
            <stroke
                android:width="1dp"
                android:color="#C1C2C1"/>
